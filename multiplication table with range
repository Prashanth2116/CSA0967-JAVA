import java.util.Scanner;
class table {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number: ");
        int num = sc.nextInt();
        System.out.print("Enter the range (m, n): ");
        int m = sc.nextInt();
        int n = sc.nextInt();
        sc.close();

        for (int i = m; i <= n; i++) {
            System.out.println(num + " x " + i + " = " + num * i);
        }
    }
}
