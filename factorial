import java.util.Scanner;

class factorial {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    System.out.print("Enter a number: ");
    int n = input.nextInt();
    int result = 1;
    for (int i = 1; i <= n; i++) {
      result *= i;
    }
    System.out.println("The factorial of " + n + " is " + result);
  }
}
