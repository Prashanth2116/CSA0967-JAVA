import java.util.Scanner;
class numpattern {
  public static void main(String[] args) {
    int rows = 4, num = 1;

    for (int i = 1; i <= rows; i++) {
      for (int j = 1; j <= i; j++) {
        System.out.print((int) Math.pow(num, 2) + " ");
        num++;
      }
      System.out.println();
    }
  }
}
